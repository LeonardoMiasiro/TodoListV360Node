{
  "$schema": "https://json.schemastore.org/tsconfig",
  "_version": "20.1.0",
  "compilerOptions": {
    "lib": [
      "es2023"
    ],
    "module": "node16",
    "target": "es2022",
    "strict": true,
    "baseUrl": "./",
    "rootDir": "./src",
    "outDir": "./dist",
    "strictPropertyInitialization": false,
    "skipLibCheck": true,
    "moduleResolution": "node16",
    "paths": {
      "@/": ["./src/"]
    }
  }
}